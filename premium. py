### SANDI ###

def menghasilkan (teks):
	hasil=[]
	ct = buka('negara.txt', 'r').read()
	untuk saya di text.split(" "):
		jika len(i)<3:
			melanjutkan
		lain:
			i=i.bawah()
			jika len(i)==3 atau len(i)==4 atau len(i)==5:
				hasil.tambahkan(i+"123")
				hasil.tambahkan(i+"12345")
			lain:
				hasil.tambahkan(i+"123")
				hasil.tambahkan(i+"12345")
				hasil.tambahkan(i)
				jika "id" di ct:
					result.append("sayang")
					result.append("bismillah")
					result.append("anjing")
					result.append("123456")
				elif "bd" di ct:
					result.append("786786")
					result.append("000786")
					result.append("102030")
					result.append("556677")
				elif "pk" di ct:
					result.append("pakistan")
					result.append("786786")
					result.append("000786")
				elif "kita" di ct:
					result.append("123456")
					result.append("qwerty")
					result.append("aku sayang kamu")
					result.append("password")
	mengembalikan hasil

