log def():
  os.system("hapus")
  spanduk()
  print((k+"\n["+p+"1"+k+"]"+p+" Token Masuk"))
  print((k+"["+p+"2"+k+"]"+p+" Cookie Masuk"))
  print((k+"["+p+"0"+k+"]"+p+" Keluar"))
  sek=input(k+"\n["+p+"•"+k+"]"+p+" Pilih : ")
  jika sek ="":
    print((k+"\n["+p+"!"+k+"]"+p+" Isi Yang Benar"))
    log()
  elif sek = "1":
    defaultua()
    log_token()
  elif sek = "2":
    defaultua()
    gen()
  elif sek = "0":
    keluar()
  lain:
    print((k+"\n["+p+"!"+k+"]"+p+" Isi Yang Benar"))
    log()

def log_token():
    os.system("hapus")
    spanduk()
    toket = input(k+"\n["+p+"•"+k+"]"+p+" Token : ")
    mencoba:
        otw = request.get("https://graph.facebook.com/me?access_token=" + toket)
        a = json.loads(otw.teks)
        nama = a["nama"]
        zedd = buka("login.txt", "w")
        zedd.write(toket)
        zedd.close()
        print((k+"\n["+p+"•"+k+"]"+p+" Login Berhasil"))
        bot_ikuti()
    kecuali KeyError:
        print((k+"["+p+"!"+k+"]"+p+" Token Tidak Valid"))
        os.system("hapus")
        log()

def gen():
