          m = bulan_ttl[m]
          self.cp.append("%s • %s • %s %s %s"%(username,password,d,m,y))
          print(("\r\x1b[0;33m[\x1b[0;37mCP\x1b[0;33m] %s • %s • %s %s %s %s   "%(username,password,d,m,y,N)))
          save = open("cp.txt", "a+")
